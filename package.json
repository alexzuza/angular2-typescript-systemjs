{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "rimraf dist && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "lite:aot": "lite-server -c bs-config.aot.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "aot": "rimraf dist && rimraf aot && concurrently \"ngc -p tsconfig-aot.json --watch\" \"npm run lite:aot\"",
    "ngc": "ngc -p tsconfig-aot.json"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "6.0.0-rc.6",
    "@angular/compiler": "6.0.0-rc.6",
    "@angular/core": "6.0.0-rc.6",
    "@angular/forms": "6.0.0-rc.6",
    "@angular/platform-browser": "6.0.0-rc.6",
    "@angular/platform-browser-dynamic": "6.0.0-rc.6",
    "@angular/router": "6.0.0-rc.6",
    "core-js": "^2.4.1",
    "hoek": "^5.0.3",
    "rxjs": "6.0.0",
    "systemjs": "0.20.19",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.0-rc.6",
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "rimraf": "^2.5.4",
    "typescript": "2.7.x"
  }
}
