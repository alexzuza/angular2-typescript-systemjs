{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "rimraf dist && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "lite:aot": "lite-server -c bs-config.aot.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "aot": "rimraf dist && rimraf aot && concurrently \"ngc -p tsconfig-aot.json\" \"npm run lite:aot\""
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "5.0.0-beta.3",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "5.0.0-beta.3",
    "@angular/compiler": "5.0.0-beta.3",
    "@angular/core": "5.0.0-beta.3",
    "@angular/forms": "5.0.0-beta.3",
    "@angular/http": "5.0.0-beta.3",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "5.0.0-beta.3",
    "@angular/platform-browser-dynamic": "5.0.0-beta.3",
    "@angular/router": "5.0.0-beta.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "systemjs": "0.19.41",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.0.0-beta.3",
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "rimraf": "^2.5.4",
    "typescript": "~2.3.2"
  }
}
